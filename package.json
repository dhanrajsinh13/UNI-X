    "postinstall": "echo 'MongoDB setup complete'",
    "socket:dev": "node -r dotenv/config socket-server.js dotenv_config_path=.env.local",
    "socket:prod": "cd socket-server && set NODE_ENV=production && npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "sync:env": "powershell -ExecutionPolicy Bypass -File sync-env.ps1",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.10",
