    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.22",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "express": "^5.2.1",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^6.20.0",
    "multer": "^2.0.2",
    "next": "15.5.7",
    "next-auth": "^4.24.11",
